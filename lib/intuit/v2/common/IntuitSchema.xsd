                        <xs:annotation>
                            <xs:documentation>        Product: QBO        Description: Name of the
                                company.        Product: QBW         Description: Name of the
                                company.[br /]Data Services max. length: 1024 characters.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="CompanySignUpDateTime" type="xs:dateTime">
