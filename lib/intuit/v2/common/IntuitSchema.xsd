    <xs:complexType name="PayrollItem">
        <xs:annotation>
            <xs:documentation>     Product: QBW      Description: A payroll item (wage-based) is
                used to track individual amounts on a paycheck for wage, salary, commission, or
                bonus. You can assign these payroll items to various accounts. For non-wage based
                payroll items, such as taxes, saving plan (401K) contributions, or direct-deposit
                amounts, see [a
                href="http://ipp.developer.intuit.com/0010_Intuit_Partner_Platform/0050_Data_Services/0500_QuickBooks_Windows/0600_Object_Reference/PayrollNonWageItem"]PayrollNonWageItem[/a].
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="CdmBase">
                <xs:sequence>
                    <xs:element form="qualified" minOccurs="0" name="Name" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW           Description: User
                                recognizable name for the Payroll Item.[br /]Max. length: 31
                                characters.[br /]Required for the Create request.         Required:
                                QBW        </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="Active" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: Whether or
                                not the payroll item is active. Inactive payroll items may be hidden
                                from display and may not be used on financial transactions.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="Type" type="PayrollItemTypeEnum">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW          Description: The type
                                of wage line in payroll, defined in the PayrollItemTypeEnum.[br /]
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PayrollNonWageItem">
        <xs:annotation>
            <xs:documentation>     Product: QBW      Description: A non-wage payroll item is used to
                track individual amounts on a paycheck for taxes, saving plan (401K) contributions,
                direct-deposit amounts - items other than wage, salary, commission, or bonus. You
                can assign these payroll items to various accounts. For wage-based payroll items,
                see [a
                href="http://ipp.developer.intuit.com/0010_Intuit_Partner_Platform/0050_Data_Services/0500_QuickBooks_Windows/0600_Object_Reference/PayrollItem"]PayrollItem[/a].
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="CdmBase">
                <xs:sequence>
                    <xs:element form="qualified" minOccurs="0" name="Name" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW           Description: User
                                recognizable name for the Payroll Item.[br /]Max. length: 31
                                characters.[br /]Required for the Create request.         Required:
                                QBW        </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="Active" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: Whether or
                                not the payroll non-wage item is active. Inactive payroll non-wage
                                items may be hidden from display and may not be used on financial
                                transactions.        </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="Type" type="PayrollNonWageItemTypeEnum">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW          Description: The type
                                of non-wage line in payroll, defined in the
                                PayrollNonWageItemTypeEnum.[br /]        </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="ExpenseAccountId" type="IdType">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: The Expense
                                account from which the non-wage item is paid.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="ExpenseAccountName" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: The Expense
                                account from which the non-wage item is paid.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="LiabilityAccountId" type="IdType">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: The
                                liability account to which the non-wage item is paid.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element form="qualified" minOccurs="0" name="LiabilityAccountName" type="xs:string">
                        <xs:annotation>
                            <xs:documentation>         Product: QBW         Description: The
                                liability account from which the non-wage item is paid.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
