        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TimeActivityTypeEnum">
        <xs:annotation>
            <xs:documentation>     Product: QBW, QBO     Description: Enumeration of TimeActivity
                person types.    </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Employee"></xs:enumeration>
            <xs:enumeration value="Vendor"></xs:enumeration>
        </xs:restriction>
