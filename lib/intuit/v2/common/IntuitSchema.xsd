        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SalesTermTypeEnum">
        <xs:restriction base="xs:string">
            <xs:enumeration value="DateDriven"></xs:enumeration>
            <xs:enumeration value="Standard"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="separatorEnum">
