    <xs:complexType name="AddRequest">
        <xs:annotation>
            <xs:documentation>         Product: QBW         Description: Request type to add new
                data to the system       </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractRequest">
                <xs:sequence>
                    <xs:element ref="CdmObject"></xs:element>
                </xs:sequence>
                <xs:attribute default="false" name="Draft" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>               Product: QBW               Description: By
                            specifying Draft=true in the Add or Mod request, the object is saved to
                            IDS, but will not be synched.A draft object saved to IDS in a Mod
                            request can subsequently be modified or reverted, but not deleted.
                            Specifies whether the given object is ready to be written back to
                            QuickBooks. The default value is false, meaning the object is ready to
                            be written to QB.             </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute default="false" name="FullResponse" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>               Product: QBW               Description:
                            Whether the entire object should be returned in the response to the
                            Create, Update or Revert request. The default value is false and the
                            response will contain the status of the request and Id of the object.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ModRequest">
        <xs:annotation>
            <xs:documentation>         Product: QBW         Description: Request type to modify
                existing data in the system       </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractRequest">
                <xs:sequence>
                    <xs:element ref="CdmObject"></xs:element>
                </xs:sequence>
                <xs:attribute default="false" name="Draft" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>               Product: QBW               Description:
                            Whether the given object is ready to be written back to QuickBooks. The
                            default value is false, meaning the object is ready to be written to QB.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute default="false" name="Sparse" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>               Product: QBW               Description:
                            Whether the object provided in the Mod request contains the entire
                            object or only partial data. If only the partial data is provided
                            (sparse = true), when writing the data back to QuickBooks, the given
                            data will be merged with the data from the provisional data base. The
                            missing elements will not be removed from the object.
                        </xs:documentation>
                    </xs:annotation>
                </xs:attribute>
                <xs:attribute default="false" name="FullResponse" type="xs:boolean">
                    <xs:annotation>
                        <xs:documentation>               Product: QBW               Description:
                            Whether the entire object should be returned in the response to the Add,
                            Mod or Revert request. The default value is false and the response will
                            contain the status of the request and Id of the object.
                        </xs:documentation>
