    <xs:simpleType name="AccountSubtypeEnum">
        <xs:annotation>
            <xs:documentation>           Product: QBO           Description: Not Supported By
                QuickBooks Online, instead use QboAccountDetailTypeEnum.           Product: QBW
                Description: Enumeration of Account sub-types used to categorize accounts in
                QuickBooks.       </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="Bank">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Checking, savings, and money market accounts. Add one bank account for every
                        account your company has at a bank or other financial institution. You can
                        also use this type for petty cash.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Accounts Receivable">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Transactions related to the customers that owe you money, including
                        invoices, payments, deposits of payments, refunds, credit memos, and
                        statements. Most companies have only one A/R account.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other Current Asset">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Assets that are likely to be converted to cash or used up within one year,
                        such as petty cash, notes receivable due within a year, prepaid expenses,
                        and security deposits.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Fixed Asset">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Depreciable assets your company owns that aren't likely to be converted into
                        cash within a year, such as equipment or furniture.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other Asset">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: Any
                        asset that is neither a current asset nor a fixed asset, such as long-term
                        notes receivable.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Accounts Payable">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Transactions related to money you owe, including bills, bill payments, and
                        any credit you have with vendors. See also current and long-term liability
                        accounts.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Credit Card">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Credit card purchases, bills, and payments.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other Current Liability">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Liabilities that are scheduled to be paid within one year, such as sales
                        tax, payroll taxes, accrued or deferred salaries, and short-term loans.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Long Term Liability">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Liabilities such as loans or mortgages scheduled to be paid over periods
                        longer than one year.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Equity">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        Owner's equity, including capital investment, drawings, and retained
                        earnings.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Income">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: The
                        main source of money coming into your company.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Cost of Goods Sold">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: The
                        cost of goods and materials held in inventory and then sold.
                    </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Expense">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: Money
                        that's leaving your company.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other Income">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: Money
                        received for something other than normal business operations, such as
                        interest income.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Other Expense">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description: Money
                        spent on something other than normal business operations, such as corporate
                        taxes.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Non-Posting">
                <xs:annotation>
                    <xs:documentation>                Product: QBW                Description:
                        QuickBooks sets up nonposting accounts for purchase orders and estimates if
                        you turn on those features.            </xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
